<% include ./_header %>
<% include ./_flash %>

<% for (let i=0; i<posts.length; i++) {%>
    <div class="post">
        <h2><%= posts[i].title %></h2>
        <span class="meta">By <%= posts[i].username %> on <%= posts[i].date %></span>
        <div class="post_content">
            <%= posts[i].content%>
        </div>
    </div>
<% }%>


<% include ./_footer %>